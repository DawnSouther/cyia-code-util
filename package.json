{
  "name": "cyia-code-util",
  "version": "1.3.7",
  "description": "代码工具",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "ts-node -P ./tsconfig.spec.json ./node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "deploy": "typedoc --out docs src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wszgrcy/cyia-code-util.git"
  },
  "author": "wszgrcy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wszgrcy/cyia-code-util/issues"
  },
  "homepage": "https://github.com/wszgrcy/cyia-code-util#readme",
  "devDependencies": {
    "@commitlint/cli": "^9",
    "@commitlint/config-conventional": "^9",
    "@types/inquirer": "^7.3.1",
    "@types/jasmine": "^3.6.2",
    "@types/node": "^14.14.20",
    "husky": "^4.3.7",
    "jasmine": "^3.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@angular/compiler": "^12.2.9",
    "css-what": "^4.0.0",
    "inquirer": "^7.3.3",
    "jsonc-parser": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
